<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="120dp"
    tools:ignore="Deprecated">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="8dp"
        android:layout_y="19dp"
        android:text="@string/update_pending_order_order_id"
        android:textColor="@color/black"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="20dp" />

    <TextView
        android:id="@+id/update_pending_order_order_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="27dp"
        android:layout_y="47dp"
        android:text="1"
        android:textColor="@color/black"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="70dp"
        android:layout_y="5dp"
        android:text="@string/update_pending_order_start_time"
        android:textColor="@color/black"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="70dp"
        android:layout_y="30dp"
        android:text="@string/update_pending_order_ready_time"
        android:textColor="@color/black"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="70dp"
        android:layout_y="55dp"
        android:text="@string/update_pending_order_pickup_time"
        android:textColor="@color/black"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="30dp"
        android:layout_y="90dp"
        android:text="@string/update_pending_order_status"
        android:textColor="@color/black"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="20dp" />

    <TextView
        android:id="@+id/update_pending_order_start_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="144dp"
        android:layout_y="5dp"
        android:text="May 14, 2018 10:00:00 AM"
        android:textColor="@color/black"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="20dp" />

    <TextView
        android:id="@+id/update_pending_order_ready_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="153dp"
        android:layout_y="30dp"
        android:text="May 14, 2018 10:00:00 AM"
        android:textColor="@color/black"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="20dp" />

    <TextView
        android:id="@+id/update_pending_order_pickup_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="158dp"
        android:layout_y="54dp"
        android:text="May 14, 2018 10:00:00 AM"
        android:textColor="@color/black"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="20dp" />

    <Spinner
        android:id="@+id/update_pending_order_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="82dp"
        android:layout_y="89dp"
        android:entries="@array/status_arrays"
        android:spinnerMode="dropdown"
        android:textColor="@color/black"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="20dp" />

    <Button
        android:id="@+id/update_pending_order_detail"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:layout_x="317dp"
        android:layout_y="79dp"
        android:text="@string/update_pending_order_detail"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="210dp"
        tools:layout_editor_absoluteY="71dp" />

</AbsoluteLayout>